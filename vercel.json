{
  "version": 2,
  "buildCommand": "echo 'No build needed - serving TypeScript directly'",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "framework": null,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server/index.ts"
    }
  ]
}
